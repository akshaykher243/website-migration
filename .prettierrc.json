{
  "tabWidth": 2,
  "endOfLine": "lf",
  "semi": true,
  "printWidth": 80,
  "singleQuote": true,
  "trailingComma": "es5",
  "quoteProps": "preserve",
  "bracketSpacing": true,
  "proseWrap": "never",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^dotenv$", "^./tracer$", "^.*mock/vbo-api$", "<THIRD_PARTY_MODULES>", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
